<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY health "http://sensormeasurement.appspot.com/health#" >
]>
<rdf:RDF xmlns="&health;"
     xml:base="&health;"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:health="&health;"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
     

     
    <owl:Ontology rdf:about="&health;">
        <rdfs:comment>Health ontology </rdfs:comment>
    </owl:Ontology>
 
 	<owl:Class rdf:ID="Symptom">		
		<rdfs:label xml:lang="en">Symptom</rdfs:label>
		<rdfs:label xml:lang="fr">Symptome</rdfs:label>

	</owl:Class>
 
 	<owl:DatatypeProperty rdf:ID="hasSymptom">
		<rdfs:label xml:lang="en">hasSymptom</rdfs:label>

	</owl:DatatypeProperty>
 
</rdf:RDF>



