<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
         <!ENTITY rdfs	"http://www.w3.org/2000/01/rdf-schema#">
         <!ENTITY owl   "http://www.w3.org/2002/07/owl#" >
         <!ENTITY naturopathy "http://sensormeasurement.appspot.com/naturopathy#">
         <!ENTITY foaf "http://xmlns.com/foaf/0.1/"> 
         <!ENTITY xsd   "http://www.w3.org/2001/XMLSchema#">  
         <!ENTITY food_smart_product "http://kmi.open.ac.uk/projects/smartproducts/ontologies/food.owl#"> 
         <!ENTITY cito "http://purl.org/spar/cito/">
         <!ENTITY dcterms "http://purl.org/dc/terms/">
         <!ENTITY sweet_season "http://sweet.jpl.nasa.gov/2.3/reprTimeSeason.owl">
         
         <!ENTITY obo "http://purl.obolibrary.org/obo/">    
         <!ENTITY emoca "http://ns.inria.fr/emoca#" >
         
         <!ENTITY Person_SH_Ontology "http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#">   
         <!ENTITY OntoReaChir "http://www.yonamoke.com/ics/en/ressources">
         <!ENTITY registry "http://ontology-for-registry-of-children-with-special-needs.googlecode.com/svn-history/r23/trunk/Registry3.4.4.owl">
         
]> 
<rdf:RDF
        xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:rdfs="&rdfs;"
        xmlns="&naturopathy;"
        xmlns:food_smart_product="&food_smart_product;"
        xml:base="&naturopathy;"
        xmlns:naturopathy="&naturopathy;" 
        xmlns:obo="&obo;"
        xmlns:emoca="&emoca;"
        xmlns:sweet_season="&sweet_season;"
        xmlns:Person_SH_Ontology="&Person_SH_Ontology;"
        xmlns:registry="&registry;"
        xmlns:OntoReaChir="&OntoReaChir;"
        xmlns:foaf="&foaf;"
        xmlns:cito="&cito;"
        xmlns:owl="&owl;"
        xmlns:dcterms="&dcterms;"
		xmlns:dc="http://purl.org/dc/elements/1.1/"
        xmlns:xsd="&xsd;"
       	xmlns:vann="http://purl.org/vocab/vann/"
       	xmlns:cc="http://creativecommons.org/ns#"
       	xmlns:vs="http://www.w3.org/2003/06/sw-vocab-status/ns#">
       	

	<owl:Ontology rdf:about="&naturopathy;">
		<rdf:type rdf:resource="http://purl.org/vocommons/voaf#Vocabulary"/>
		<dc:title xml:lang="en">The naturopathy ontology</dc:title>
		<dc:description xml:lang="en">The naturopathy ontology describes relationships between foods, their nutrients, their color, the mood, the diseases, the diets and the allergies.</dc:description>
		<dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Amelie Gyrard</dc:creator>
		<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2013-09-12</dcterms:issued> 
 		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2014-07-04</dcterms:modified> 
		<owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.1</owl:versionInfo>
		<vs:term_status>Work in progress</vs:term_status>
		<vann:preferredNamespacePrefix>naturopathy</vann:preferredNamespacePrefix> 
	    <vann:preferredNamespaceUri>http://sensormeasurement.appspot.com/naturopathy#</vann:preferredNamespaceUri>
	    <foaf:homepage>http://sensormeasurement.appspot.com/?p=ontologies</foaf:homepage>
	    <rdfs:seeAlso>
		    <foaf:Document rdf:about="http://goo.gl/Hb1bG4">
	   				<rdfs:label>The naturopathy ontology documentation</rdfs:label>
	   		</foaf:Document>
   		</rdfs:seeAlso>   
   		<rdfs:seeAlso>
		    <foaf:Document rdf:about="http://sensormeasurement.appspot.com/naturopathy#">
	   				<rdfs:label>The naturopathy ontology code (RDF/XML)</rdfs:label>
	   		</foaf:Document>
   		</rdfs:seeAlso> 		
   		<rdfs:seeAlso>
		    <foaf:Document rdf:about="http://goo.gl/c7Hbvq">
	   				<rdfs:label>The naturopathy dataset documentation</rdfs:label>
	   		</foaf:Document>
   		</rdfs:seeAlso>
   		<rdfs:seeAlso>
		    <foaf:Document rdf:about="http://sensormeasurement.appspot.com/naturopathy-dataset">
	   				<rdfs:label>The naturopathy dataset code (RDF/XML)</rdfs:label>
	   		</foaf:Document>
   		</rdfs:seeAlso> 
	    <cito:citesAsDataSource>
	    	<foaf:Document rdf:about="http://rhizomik.net/ontologies/2008/05/emotionsonto.owl#">
   				<rdfs:label>Inspired by "Towards an ontology for describing emotions". [Lopez et al. 2008]</rdfs:label>
   				<dcterms:description xml:lang="en">The naturopathy ontology has been inspired by this emotion ontology for the sensor measurements such as heart rate, galvanic skin response (GSR) and electroencephalographic (ECG).</dcterms:description>
   			</foaf:Document>
   		</cito:citesAsDataSource> 
		<cito:citesAsDataSource>
  			<foaf:Document rdf:about="http://emotion-ontology.googlecode.com/svn/trunk/ontology/">
   				<rdfs:label>Inspired by "The emotion ontology: Enabling Interdisciplinary Research in the Affective Sciences" [Hastings et al. 2011]. </rdfs:label>
   				<dcterms:description xml:lang="en">The naturopathy ontology has been inspired by this emotion ontology to describe the different emotions.</dcterms:description>	
   			</foaf:Document>
		</cito:citesAsDataSource>
		<cito:citesAsDataSource>
  			<foaf:Document rdf:about="">
   				<rdfs:label>Inspired by "Towards the Mental Health Ontology". [Hadzic et al. 2008]. </rdfs:label>
   				<dc:description xml:lang="en">
   					The naturopathy ontology has been inspired by this emotion ontology to describe the relationships between nutrients, food and emotions.
   					An example is that chocolate contains vitamin B which is recommended when you are sad or for the stress.
   				</dc:description>
   			</foaf:Document>
		</cito:citesAsDataSource>
		<cito:citesAsDataSource>
  			<foaf:Document rdf:about="">
   				<rdfs:label>Inspired by the ontology fruitsTropicaux. </rdfs:label>
   				<dcterms:description xml:lang="en">The naturopathy ontology has been inspired by this ontology for the property hasVitamin, hasColor, hasMineral.</dcterms:description>	
   			</foaf:Document>
		</cito:citesAsDataSource>
		<cito:citesAsDataSource>
  			<foaf:Document rdf:about="http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#">
   				<rdfs:label>Inspired by the person sh ontology</rdfs:label>
   				<dcterms:description xml:lang="en">The naturopathy ontology has been inspired by this ontology for the concepts Allergy, Disease (Cancer, Asthma, Arthrisis)and the property wearSensor.</dcterms:description>	
   			</foaf:Document>
		</cito:citesAsDataSource>
		<cito:citesAsDataSource>
  			<foaf:Document rdf:about="http://ontology-for-registry-of-children-with-special-needs.googlecode.com/svn-history/r23/trunk/Registry3.4.4.owl">
   				<rdfs:label>Inspired by the registry ontology</rdfs:label>
   				<dcterms:description xml:lang="en">The naturopathy ontology has been inspired by this ontology for the person description. </dcterms:description>	
   			</foaf:Document>
		</cito:citesAsDataSource>
		<cito:citesAsDataSource>
  			<foaf:Document rdf:about="http://www.personal.psu.edu/wxy119/hospital_ontology.owl">
   				<rdfs:label>Inspired by the hospital ontology</rdfs:label>
   				<dcterms:description xml:lang="en">The naturopathy ontology has been inspired by this ontology for the diseases (stomach pain, stroke, hypertension, heart attack, headache, cough disease, appendictis).</dcterms:description>	
   			</foaf:Document>
		</cito:citesAsDataSource>
		<cito:citesAsDataSource>
  			<foaf:Document rdf:about="http://ns.inria.fr/emoca/">
   				<rdfs:label>Inspired by the emoca ontology [Berthelon et al.]</rdfs:label>
   				<dcterms:description xml:lang="en">The naturopathy ontology has been inspired by this ontology for description of six emotions.</dcterms:description>	
   			</foaf:Document>
		</cito:citesAsDataSource>
		<cito:citesAsDataSource>
  			<foaf:Document rdf:about="http://hmi.ewi.utwente.nl/verslagen/capita-selecta/CS-Nijdam-Niels.pdf">
   				<rdfs:label>Inspired by "Mapping Emotion to color" [Nijdam et al. 2009]</rdfs:label>
   				<dc:description xml:lang="en">Relationships beetween emotion and colors.</dc:description>	
   			</foaf:Document>
		</cito:citesAsDataSource>
	</owl:Ontology>
	
	
	
	<owl:Class rdf:ID="Food">		
		<rdfs:label xml:lang="en">Food</rdfs:label>		
		<owl:equivalentClass rdf:resource="&food_smart_product;FoodOrDrinkItem"/>
	</owl:Class> 		
	
	<owl:Class rdf:ID="Herb">		
		<rdfs:label xml:lang="en">Herb</rdfs:label>
		<rdfs:subClassOf rdf:resource="#Food"/>		
	</owl:Class> 
	
	
  
	
	
</rdf:RDF>