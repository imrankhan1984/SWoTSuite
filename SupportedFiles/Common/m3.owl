<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
         <!ENTITY rdfs	"http://www.w3.org/2000/01/rdf-schema#">
         <!ENTITY owl   "http://www.w3.org/2002/07/owl#">
         <!ENTITY dcterms "http://purl.org/dc/terms/">
         <!ENTITY dc "http://purl.org/dc/elements/1.1/">
         
         <!ENTITY dul "http://www.loa-cnr.it/ontologies/DUL.owl#">
         <!ENTITY spitfire "http://spitfire-project.eu/ontology/ns/">    
         <!ENTITY openIoT "http://openiot.eu/ontology/ns/">          
               

         <!ENTITY foaf "http://xmlns.com/foaf/0.1/">
         <!ENTITY xsd   "http://www.w3.org/2001/XMLSchema#">          
         <!ENTITY m3	"http://sensormeasurement.appspot.com/m3#"> 
         <!ENTITY naturopathy "http://sensormeasurement.appspot.com/naturopathy#"> 
         <!ENTITY stac	"http://securitytoolbox.appspot.com/stac#">  
         <!ENTITY home "http://sensormeasurement.appspot.com/home#"> 
         <!ENTITY food_smart_product "http://kmi.open.ac.uk/projects/smartproducts/ontologies/food.owl#"> 
      	 <!ENTITY shw "http://paul.staroch.name/thesis/SmartHomeWeather.owl#"> 
        
        
        <!ENTITY cose "http://casas.wsu.edu/owl/cose.owl#">
        <!ENTITY ssn "http://purl.oclc.org/NET/ssnx/ssn#">
        
        <!ENTITY muo "http://purl.oclc.org/NET/muo/muo#">
        <!ENTITY qudt "http://qudt.org/schema/qudt#">
        <!ENTITY qudt_unit "http://data.qudt.org/qudt/owl/1.0.0/unit.owl#">        
        <!ENTITY ucum "http://idi.fundacionctic.org/muo/ucum-instances.owl">
        
        <!ENTITY aws "http://purl.oclc.org/NET/ssnx/meteo/aws#"> 
        <!ENTITY wgs84_pos "http://www.w3.org/2003/01/geo/wgs84_pos#">
        <!ENTITY sweet_unit "http://sweet.jpl.nasa.gov/ontology/units.owl#">
        <!ENTITY weather "http://archive.knoesis.org/research/semsci/application_domain/sem_sensor/ont/weather.owl">
        <!ENTITY semsor_weather "http://archive.knoesis.org/research/semsci/application_domain/sem_sensor/ont/sensor-observation.owl">
        <!ENTITY ontoSensor "http://mmisw.org/ont/univmemphis/sensor">    
        <!ENTITY emotion "http://rhizomik.net/ontologies/2008/05/emotionsonto.owl#">         
        <!ENTITY dogont "http://elite.polito.it/ontologies/dogont.owl#">
        <!ENTITY dbpedia "http://dbpedia.org/ontology/">
        <!ENTITY movie "http://www.movieontology.org/2010/01/movieontology.owl">   
        <!ENTITY mo "http://purl.org/ontology/mo/">   

        
        <!ENTITY emoca "http://ns.inria.fr/emoca#" >
        <!ENTITY txn	"http://lod.taxonconcept.org/ontology/txn.owl#"> 
        <!ENTITY voaf	"http://purl.org/vocommons/voaf#"> 
        <!ENTITY vann	"http://purl.org/vocab/vann/"> 
        
         
]> 
<rdf:RDF
        xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:rdfs="&rdfs;"
        xmlns:voaf="&voaf;"
        xml:base="&m3;"
        xmlns:m3="&m3;"
        xmlns:cose="&cose;"
        xmlns:dul="&dul;"
        xmlns:spitfire="&spitfire;"
        xmlns:openIoT="&openIoT;"
        xmlns:shw="&shw;"
        xmlns:txn="&txn;"
        xmlns:naturopathy="&naturopathy;"
        xmlns:stac="&stac;"
        xmlns:home="&home;"
        xmlns:foaf="&foaf;"
        xmlns:dcterms="&dcterms;"
        xmlns:dbpedia="&dbpedia;"
        xmlns:food_smart_product="&food_smart_product;"
        xmlns:ssn="&ssn;"
        
       	xmlns:muo="&muo;"
        xmlns:qudt="&qudt;"
        xmlns:qudt_unit="&qudt_unit;"
        xmlns:ucum="&ucum;"
        
        xmlns:emoca="&emoca;"
        xmlns:ontoSensor="&ontoSensor;"
        xmlns:sweet_unit="&sweet_unit;"
        xmlns:weather="&weather;"
        xmlns:aws="&aws;"
        xmlns:wgs84_pos="&wgs84_pos;"
        xmlns:dogont="&dogont;"
        xmlns:owl="&owl;"
        xmlns:mo="&mo;"
        xmlns:movie="&movie;"
		xmlns:dc="&dc;"
        xmlns:xsd="&xsd;"
       	xmlns:vann="&vann;"
       	xmlns:cc="http://creativecommons.org/ns#"
       	xmlns:vs="http://www.w3.org/2003/06/sw-vocab-status/ns#">
       	
<!-- W3C SSN ontology:
What is the difference between ssn:observes and ssn:observedProperty? -->

	<owl:Ontology rdf:about="&m3;">
		<rdf:type rdf:resource="&voaf;Vocabulary"/>
		<dc:title xml:lang="en">The Machine-to-Machine Measurement (M3) Ontology</dc:title>
		<dc:description xml:lang="en">The Machine-to-Machine Measurement (M3) Ontology is a unified language, nomenclature, dictionnary which enables semantically annotating heterogeneous IoT data produced by heterogeneous devices measurements (sensors, RFID tags, etc.). We classify all devices, their measurements and the feature of interests (health, smart home, smart kitchen, environmental monitoring, etc.). By combining data from heterogeneous areas, we could propose new kinds of applications.</dc:description>
		<dc:creator>
  			<foaf:Person rdf:about="mailto:gyrard@eurecom.fr">
   			<foaf:name>Amelie Gyrard</foaf:name>
  			</foaf:Person>
		</dc:creator>
		<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2013-02-21</dcterms:issued> 
 		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-05-26</dcterms:modified> 
		<owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.2</owl:versionInfo>

		<vs:term_status>Work in progress</vs:term_status>
		<vann:preferredNamespacePrefix>m3</vann:preferredNamespacePrefix> 
	    <vann:preferredNamespaceUri>http://sensormeasurement.appspot.com/m3#</vann:preferredNamespaceUri>
	</owl:Ontology>



	<owl:Class rdf:ID="BodyTemperature">		
		<rdfs:label xml:lang="en">Body Temperature</rdfs:label>
		<rdfs:subClassOf rdf:resource="#Temperature"/>
	</owl:Class> 
	
	
	<owl:Class rdf:ID="Temperature">		
		<rdfs:label xml:lang="en">Temperature</rdfs:label>
		<rdfs:comment xml:lang="en">By defaut, Air temperature</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#MeasurementType"/>
		<owl:equivalentClass rdf:resource="&semsor_weather;TemperatureProperty"/>
		<owl:equivalentClass rdf:resource="&aws;air_temperature"/>
		<rdfs:seeAlso rdf:resource="&openIoT;Temperature"/>
		<rdfs:subClassOf>
			<owl:Restriction>
            	<owl:onProperty rdf:resource="#hasUnit"/>
             	<owl:someValuesFrom rdf:resource="#DegreeKelvin"/>
        	</owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
			<owl:Restriction>
            	<owl:onProperty rdf:resource="#hasUnit"/>
             	<owl:someValuesFrom rdf:resource="#DegreeCelsius"/>
        	</owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
			<owl:Restriction>
            	<owl:onProperty rdf:resource="#hasUnit"/>
             	<owl:someValuesFrom rdf:resource="#DegreeFahrenheit"/>
        	</owl:Restriction>
        </rdfs:subClassOf>
	</owl:Class>
	
	
	<owl:Class rdf:ID="DegreeCelsius">		
		<rdfs:label xml:lang="en">Degree celsius</rdfs:label>
		<rdfs:subClassOf rdf:resource="#Unit"/>	
		<owl:equivalentClass rdf:resource="&sweet_unit;degreeC"/>
		<rdfs:seeAlso rdf:resource="&qudt_unit;DegreeCelsius"/>
	</owl:Class>
	
	<owl:Class rdf:ID="DegreeFahrenheit">		
		<rdfs:label xml:lang="en">Degree fahrenheit</rdfs:label>
		<rdfs:subClassOf rdf:resource="#Unit"/>	
		<owl:equivalentClass rdf:resource="&sweet_unit;degreeF"/>
		<owl:equivalentClass rdf:resource="&weather;fahrenheit"/>
	</owl:Class> 
	
	
	<owl:Class rdf:ID="MeasurementType">		
		<rdfs:label xml:lang="en">Measurement Type</rdfs:label>
		<owl:equivalentClass rdf:resource="&semsor_weather;PropertyType"/>
	</owl:Class> 
	
	<owl:ObjectProperty rdf:ID="hasUnit">
  		<rdfs:domain rdf:resource="#Measurement"/>
 		<rdfs:range  rdf:resource="#Unit"/>
		<rdfs:label xml:lang="en">hasUnit</rdfs:label>
	</owl:ObjectProperty> 
	
		<owl:Class rdf:ID="DegreeKelvin">		
		<rdfs:label xml:lang="en">Degree Kelvin</rdfs:label>
		<rdfs:subClassOf rdf:resource="#Unit"/>	
		<owl:equivalentClass rdf:resource="&sweet_unit;kelvin"/>
	</owl:Class> 
	
	<owl:Class rdf:ID="DegreeCelsius">		
		<rdfs:label xml:lang="en">Degree celsius</rdfs:label>
		<rdfs:subClassOf rdf:resource="#Unit"/>	
		<owl:equivalentClass rdf:resource="&sweet_unit;degreeC"/>
		<rdfs:seeAlso rdf:resource="&qudt_unit;DegreeCelsius"/>
	</owl:Class>
	
	<owl:Class rdf:ID="DegreeFahrenheit">		
		<rdfs:label xml:lang="en">Degree fahrenheit</rdfs:label>
		<rdfs:subClassOf rdf:resource="#Unit"/>	
		<owl:equivalentClass rdf:resource="&sweet_unit;degreeF"/>
		<owl:equivalentClass rdf:resource="&weather;fahrenheit"/>
	</owl:Class> 
	
	<owl:Class rdf:ID="Unit">		
		<rdfs:label xml:lang="en">Unit</rdfs:label>
		<rdfs:seeAlso rdf:resource="&sweet_unit;Unit"/>
		<rdfs:seeAlso rdf:resource="&muo;UnitOfMeasurement"/>
		<rdfs:seeAlso rdf:resource="&ucum;BaseUnit"/>
		<owl:equivalentClass rdf:resource="&qudt;Unit"/>
		<owl:equivalentClass rdf:resource="&spitfire;Unit"/>
	</owl:Class> 
	
	
	<owl:Class rdf:ID="Measurement">		
		<rdfs:label xml:lang="en">Measurement</rdfs:label>
		<owl:equivalentClass rdf:resource="&ssn;SensorOutput"/>
	</owl:Class> 
	
	<owl:ObjectProperty rdf:ID="hasName">
  		<rdfs:domain rdf:resource="#Measurement"/>
 		<rdfs:range  rdf:resource="#MeasurementType"/>
		<rdfs:label xml:lang="en">hasName</rdfs:label>
		<owl:equivalentProperty rdf:resource="&ssn;observedProperty"/>
	</owl:ObjectProperty> 
	
	<owl:Class rdf:ID="MeasurementType">		
		<rdfs:label xml:lang="en">Measurement Type</rdfs:label>
		<owl:equivalentClass rdf:resource="&semsor_weather;PropertyType"/>
	</owl:Class> 
	
	<owl:ObjectProperty rdf:ID="produces">
  		<rdfs:domain rdf:resource="#M2MDevice"/>
 		<rdfs:range  rdf:resource="#Measurement"/>
		<rdfs:label xml:lang="en">produces</rdfs:label>
	</owl:ObjectProperty> 
	
	<owl:Class rdf:ID="M2MDevice">		
		<rdfs:label xml:lang="en">M2MDevice</rdfs:label>
	</owl:Class> 
	
	<owl:ObjectProperty rdf:ID="observes">
  		<rdfs:domain rdf:resource="#Sensor"/>
 		<rdfs:range  rdf:resource="#FeatureOfInterest"/>
		<rdfs:label xml:lang="en">observes</rdfs:label>
		<owl:equivalentProperty rdf:resource="&ssn;observes"/>
	</owl:ObjectProperty> 
	
		<owl:Class rdf:ID="Sensor">		
		<rdfs:label xml:lang="en">Physical Sensor</rdfs:label>
		<rdfs:subClassOf rdf:resource="#M2MDevice"/>	
		<owl:equivalentClass rdf:resource="&ssn;Sensor"/>
		<owl:equivalentClass rdf:resource="&ontoSensor;Sensor"/>
	</owl:Class> 
	
	<owl:Class rdf:ID="FeatureOfInterest">		
		<rdfs:label xml:lang="en">FeatureOfInterest</rdfs:label>
		 <rdfs:domain rdf:resource="#Sensor"/>
 		<rdfs:range  rdf:resource="&xsd;string"/>
		<owl:equivalentClass rdf:resource="&ssn;FeatureOfInterest"/>
		<owl:equivalentClass rdf:resource="&semsor_weather;Observation"/>
	</owl:Class> 
	
		<owl:Class rdf:ID="M2MDevice">		
		<rdfs:label xml:lang="en">M2MDevice</rdfs:label>
	</owl:Class>	
	
	<owl:ObjectProperty rdf:ID="hasValue">
  		<rdfs:domain rdf:resource="#Measurement"/>
 		<rdfs:range  rdf:resource="#Value"/><!-- &xsd;double-->
		<rdfs:label xml:lang="en">hasValue</rdfs:label>
		<owl:equivalentProperty rdf:resource="&ssn;hasValue"/>
	</owl:ObjectProperty> 
	
	<owl:Class rdf:ID="Health">		
		<rdfs:label xml:lang="en">Healthcare</rdfs:label>
		<rdfs:subClassOf rdf:resource="#FeatureOfInterest"/>			
	</owl:Class>
	
	
</rdf:RDF>