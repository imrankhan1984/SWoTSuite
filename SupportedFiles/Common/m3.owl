	<owl:Class rdf:ID="Glucometer">		
		<rdfs:label xml:lang="en">Glucometer, Blood sugar, blood glucose level</rdfs:label>

		<rdfs:subClassOf rdf:resource="#Sensor"/>	
		<rdfs:subClassOf rdf:resource="#HealthM2MDevice"/>
