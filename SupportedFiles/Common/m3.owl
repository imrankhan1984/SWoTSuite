	<owl:Class rdf:ID="HealthM2MDevice">		
		<rdfs:label xml:lang="en">Healthcare M2M Devices (sensors or actuators)</rdfs:label>

		<rdfs:subClassOf rdf:resource="#DomainM2MDevice"/>	
	</owl:Class>
	
	<owl:Class rdf:ID="EmotionM2MDevice">		
		<rdfs:label xml:lang="en">Emotion M2M Devices (sensors or actuators)</rdfs:label>
		<rdfs:comment xml:lang="en">For instance ECG</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#DomainM2MDevice"/>	
	</owl:Class>
	
		<owl:Class rdf:ID="ActivityM2MDevice">		
		<rdfs:label xml:lang="en">Activity M2M Devices (sensors or actuators)</rdfs:label>
		<rdfs:comment xml:lang="en">For instance accelerometer</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#DomainM2MDevice"/>	
	</owl:Class>
	
	<owl:Class rdf:ID="EnergyM2MDevice">		
		<rdfs:label xml:lang="en">Energy M2M Devices (sensors or actuators)</rdfs:label>
		<rdfs:comment xml:lang="en">For instance thermometer or energy meter</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#DomainM2MDevice"/>	
	</owl:Class>
	
	<owl:Class rdf:ID="PulseOxymeter">		
		<rdfs:label xml:lang="en">Pulse Oxymeter, SpO2, Blood Oxygen Saturation Sensor</rdfs:label>
		<rdfs:comment xml:lang="en">Pulse oximetry is a method used to measure the concentration of oxygen in the blood.</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#Sensor"/>
		<rdfs:subClassOf rdf:resource="#HealthM2MDevice"/>
		<rdfs:subClassOf>
			<owl:Restriction>
            	<owl:onProperty rdf:resource="#produces"/>
             	<owl:someValuesFrom rdf:resource="#SPO2"/>
        	</owl:Restriction>
        </rdfs:subClassOf>	
	</owl:Class>
	
	<owl:Class rdf:ID="StressLevelSensor">		
		<rdfs:label xml:lang="en">Stress Level Sensor</rdfs:label>
		<rdfs:comment xml:lang="en">Stress Level Sensor</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#Sensor"/>
		<rdfs:subClassOf rdf:resource="#HealthM2MDevice"/>
		<rdfs:subClassOf rdf:resource="#EmotionM2MDevice"/>
	</owl:Class>
	
	<owl:Class rdf:ID="ECG">		
		<rdfs:label xml:lang="en">ECG or EKG (Electrocardiogram)</rdfs:label>
		<rdfs:comment xml:lang="en">ECG or EKG (Electrocardiogram)</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#Sensor"/>
		<rdfs:subClassOf rdf:resource="#HealthM2MDevice"/>
		<rdfs:subClassOf rdf:resource="#EmotionM2MDevice"/>
	</owl:Class>
	
	<owl:Class rdf:ID="BloodPressureSensor">		
		<rdfs:label xml:lang="en">Blood Pressure Sensor</rdfs:label>
		<rdfs:subClassOf rdf:resource="#Sensor"/>
		<rdfs:subClassOf rdf:resource="#HealthM2MDevice"/>
		<txn:hasImage rdf:resource="http://sensormeasurement.appspot.com/images/sensor/bloodPressure.png"/>
		<rdfs:subClassOf>
			<owl:Restriction>
            	<owl:onProperty rdf:resource="#produces"/>
             	<owl:someValuesFrom rdf:resource="#BloodPressure"/>
        	</owl:Restriction>
        </rdfs:subClassOf>
	</owl:Class>
	
	<owl:Class rdf:ID="CholesterolSensor">		
		<rdfs:label xml:lang="en">Cholesterol Sensor</rdfs:label>
		<rdfs:subClassOf rdf:resource="#Sensor"/>
		<rdfs:subClassOf rdf:resource="#HealthM2MDevice"/>
		<txn:hasImage rdf:resource="http://sensormeasurement.appspot.com/images/sensor/cholesterol.png"/>
